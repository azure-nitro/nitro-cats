    "@angular/forms": "^8.2.5",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/router": "^8.2.5",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "hammerjs": "^2.0.8",
    "imagesloaded": "^4.1.4",
